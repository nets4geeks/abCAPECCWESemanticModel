SubClassOf(:CAPEC_50 ObjectHasValue(:targetsCWE :iCWE_522))
SubClassOf(:CAPEC_50 ObjectHasValue(:targetsCWE :iCWE_640))
SubClassOf(:CAPEC_50 ObjectHasValue(:targetsCWE :iCWE_718))
SubClassOf(:CAPEC_50 DataHasValue(:scoresRequeredSkill "2"^^xsd:integer))
